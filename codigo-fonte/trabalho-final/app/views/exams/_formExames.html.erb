<%= simple_form_for(@examepaciente, :html => { :multipart => true }) do |f| %>
  <%= f.error_notification %>
  <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>

  <div class="form-inputs">
    <%= f.input :exam, label: false, input_html: { class: 'class="form-control'}%>
    <%= f.hidden_field :patient_id, :value=>@p["id"]%>
  </div>

  <div class="form-actions">
    <%= submit_tag "Salvar imagem", class: "btn btn-success" %>
  </div> <br>
<% end %>
